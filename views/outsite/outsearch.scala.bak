@(message: String)(toolName: String)
@header = {
	
}
@main("Tool Lab - Fast Lab")(message)(header) {

	<section>
                <div class ="head">
                <link rel="stylesheet" href="@routes.Assets.versioned("stylesheets/outsite/outSearch.css")">
                <script src="@routes.Assets.versioned("javascripts/outsite/jquery-1.11.3.js")" type="text/javascript"></script>
                <script src="@routes.Assets.versioned("javascripts/outsite/translate.js")" type="text/javascript"></script>
                <script src="@routes.Assets.versioned("javascripts/outsite/analytics.js")" type="text/javascript"></script>

                </div>
		<div class="container">
			<p class="text-center breadcrumb">
				<span>Tool Lab &gt;</span>
				<span class="page-title">@toolName</span>
			</p>
		</div>
		<div class="container-tool-lab">
			<h2 class="text-center article-title">輸入搜尋中文字</h2>
			<div class="text-center">
				<div class="float-left column-9">
					<input class="text-center menu-text column-12" type="text" id="keyword" placeholder="請輸入查詢圖檔名稱">
                                <input type="hidden" name="source" id="source" value="zh-CHT">
                                <input type="hidden" name="destinaion" id="destinaion" value="en">
				</div>
				<div class="float-right">
					<input class="menu-button" type="button" value="查詢" onClick="Submit()">
				</div>
				<div class="clear"></div>
			</div>
		</div>
		<div class="container-tool-lab">
			<p class="text-center article-title">搜尋結果</p>
			<div class="search-result text-center" id="search-result">
				<div class="search-result-item float-left column-4">
                       <input class="menu-button" type="button" value="Yobi3D網站" id="yobi_button" onClick="">
				</div>
				<div class="search-result-item float-left column-4">
                       <input class="menu-button" type="button" value="Yeggi網站" id="yeggi_button" onClick="">
				</div>
				<div class="search-result-item float-left column-4">

				</div>
				<div class="clear"></div>
			</div>
			<div class="text-center">
			<!--	<input class="menu-button column-12" type="button" value="搜尋"> --!>
			</div>
		</div>
	</section>

}
